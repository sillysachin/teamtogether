<div class="subhead-collapse" id="ember-toolbar">
  <div class="subhead">
    <div class="container-fluid">
      <div id="container-collapse" class="container-collapse"></div>
      <div class="row-fluid">
        <div class="span12">
          <div class="btn-toolbar" id="toolbar">
            <div class="btn-group" id="toolbar-apply">
              <button {{action "save" target="view"}} class="btn btn-small btn-success">
              <i class="icon-apply icon-white">
              </i>
              Save
              </button>
            </div>

            <div class="btn-group" id="toolbar-cancel">
              <button {{action "rollbackChanges" target="view"}} class="btn btn-small">
              <i class="icon-arrow-left ">
              </i>
              Discard unsaved settings
              </button>
            </div>

            <div class="btn-group divider">
            </div>

            <div class="btn-group" id="toolbar-help">
              <button class="btn btn-small">
              <i class="icon-question-sign">
              </i>
              Help
              </button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="container-main">
  <section id="content">
    <!-- Begin Content -->
    <div class="row-fluid">
      <div class="span12">
        <form action="/joomla3/administrator/index.php?option=com_jsonexport&amp;view=products" method="post" name="adminForm" id="adminForm">
          <div id="j-sidebar-container" class="span2">
            <div id="sidebar">
              <div class="sidebar-nav">
                <ul id="submenu" class="nav nav-list">
                  <li>
                    {{#linkTo "tables"}}Tables{{/linkTo}}
                  </li>
                  <li class="active">
                    {{#linkTo "settings"}}Settings{{/linkTo}}
                  </li>
                </ul>
                <hr>
              </div>
            </div>
          </div>
          <div id="j-main-container" class="span10">
            <div class="span10">
              <ul class="nav nav-tabs" id="configTabs">
                <li class="active"><a href="#general" data-toggle="tab">General</a></li>
              </ul>
              <div class="tab-content">
                <div class="tab-pane well active" id="general">
                  {{view App.SettingsView__GeneralView}}
                </div>
              </div>
            </div>

          </div>
        </form>
      </div>
    </div>
  </section>
</div>
